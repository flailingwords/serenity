{
    "devDependencies": {
        "@tyisi/config-eslint": "5.0.0",
        "@tyisi/config-prettier": "2.0.0",
        "@types/debug": "4.1.12",
        "@types/node": "^22.18.12",
        "eslint": "^9.38.0",
        "globals": "^16.4.0",
        "prettier": "3.6.2",
        "prettier-plugin-sh": "^0.18.0",
        "prettier-plugin-tailwindcss": "^0.7.1",
        "tsup": "^8.5.0",
        "typescript": "~5.9.3",
        "wireit": "0.14.12"
    },
    "name": "serenity-shared",
    "private": true,
    "scripts": {
        "build": "wireit",
        "prepare": "wireit"
    },
    "type": "module",
    "version": "0.0.0",
    "dependencies": {
        "@hono/zod-validator": "0.7.4",
        "debug": "4.4.3",
        "hono": "4.10.1",
        "zod": "4.1.12"
    },
    "wireit": {
        "build:shared:cjs": {
            "command": "tsup --format cjs -d dist ./src/shared.ts --dts",
            "output": [
                "dist/shared.cjs"
            ],
            "clean": true,
            "dependencies": [
                "type-check",
                "test"
            ]
        },
        "build:data-templates:cjs": {
            "command": "tsup --format cjs -d dist ./src/data-templates/index.ts --dts",
            "output": [
                "dist/data-templates.cjs"
            ],
            "clean": true,
            "dependencies": [
                "type-check",
                "test"
            ]
        },
        "build:lib:cjs": {
            "command": "tsup --format cjs -d dist ./src/lib/index.ts --dts",
            "output": [
                "dist/lib.cjs"
            ],
            "clean": true,
            "dependencies": [
                "type-check",
                "test"
            ]
        },
        "build:schemas:cjs": {
            "command": "tsup --format cjs -d dist ./src/schemas/index.ts --dts",
            "output": [
                "dist/schemas.cjs"
            ],
            "clean": true,
            "dependencies": [
                "type-check",
                "test"
            ]
        },
        "build:shared:esm": {
            "command": "tsup --format esm -d dist ./src/shared.ts --dts",
            "output": [
                "dist/shared.mjs"
            ],
            "clean": true,
            "dependencies": [
                "type-check",
                "test"
            ]
        },
        "build:data-templates:esm": {
            "command": "tsup --format esm -d dist ./src/data-templates/index.ts --dts",
            "output": [
                "dist/data-templates.mjs"
            ],
            "clean": true,
            "dependencies": [
                "type-check",
                "test"
            ]
        },
        "build:lib:esm": {
            "command": "tsup --format esm -d dist ./src/lib/index.ts --dts",
            "output": [
                "dist/lib.mjs"
            ],
            "clean": true,
            "dependencies": [
                "type-check",
                "test"
            ]
        },
        "build:schemas:esm": {
            "command": "tsup --format esm -d dist ./src/schemas/index.ts --dts",
            "output": [
                "dist/schemas.mjs"
            ],
            "clean": true,
            "dependencies": [
                "type-check",
                "test"
            ]
        },
        "build": {
            "dependencies": [
                "type-check",
                "test",
                "build:data-templates:cjs",
                "build:data-templates:esm",
                "build:lib:cjs",
                "build:lib:esm",
                "build:schemas:cjs",
                "build:schemas:esm",
                "build:shared:cjs",
                "build:shared:esm"
            ]
        },
        "prepare": {
            "dependencies": [
                "build"
            ]
        },
        "test": {
            "command": "echo TODO"
        },
        "type-check": {
            "command": "tsc --noEmit"
        }
    },
    "files": [
        "dist/**"
    ],
    "main": "./dist/shared.cjs",
    "module": "./dist/shared.js",
    "types": "./dist/shared.d.ts",
    "exports": {
        ".": {
            "types": "./dist/shared.d.ts",
            "import": "./dist/shared.js",
            "require": "./dist/shared.cjs"
        },
        "./data-templates": {
            "types": "./dist/data-templates.d.ts",
            "import": "./dist/data-templates.js",
            "require": "./dist/data-templates.cjs"
        },
        "./lib": {
            "types": "./dist/lib.d.ts",
            "import": "./dist/lib.js",
            "require": "./dist/lib.cjs"
        },
        "./schemas": {
            "types": "./dist/schemas.d.ts",
            "import": "./dist/schemas.js",
            "require": "./dist/schemas.cjs"
        },
        "./types": {
            "types": "./dist/types.d.ts",
            "import": "./dist/types.js",
            "require": "./dist/types.cjs"
        }
    }
}
