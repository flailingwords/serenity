        "@fortawesome/free-solid-svg-icons": "7.1.0",
        "@fortawesome/react-fontawesome": "3.1.0",
        "@headlessui/react": "2.2.9",
        "@heroicons/react": "2.2.0",
        "@tailwindcss/vite": "4.1.15",
        "clsx": "2.1.1",
        "debug": "4.4.3",
        "pretty-state-machine": "0.0.10",
        "react": "^19.2.0",
        "react-dom": "^19.2.0",
        "react-geolocated": "4.3.1",
        "react-interval-hook": "1.1.5",
        "react-markdown": "10.1.0",
        "serenity-shared": "workspace:*",
        "tailwindcss": "4.1.15",
        "unsplash-js": "7.0.20"
    },
    "devDependencies": {
        "@babel/eslint-parser": "7.28.4",
        "@eslint/js": "^9.38.0",
        "@storybook/addon-docs": "9.1.13",
        "@storybook/addon-links": "9.1.13",
        "@storybook/addon-onboarding": "9.1.13",
        "@storybook/react": "9.1.13",
        "@storybook/react-vite": "^9.1.13",
        "@tailwindcss/vite": "4.1.15",
        "@testing-library/jest-dom": "^6.9.1",
        "@testing-library/react": "16.3.0",
        "@tyisi/config-changelog": "2.0.0",
        "@tyisi/config-eslint": "5.0.0",
        "@tyisi/config-prettier": "2.0.0",
        "@tyisi/config-stylelint": "2.0.0",
        "@types/debug": "4.1.12",
        "@types/jest": "30.0.0",
        "@types/node": "22.18.12",
        "@types/react": "^19.2.2",
        "@types/react-dom": "^19.2.2",
        "@vitejs/plugin-react": "5.0.4",
        "autoprefixer": "10.4.21",
        "eslint": "^9.38.0",
        "eslint-plugin-react-hooks": "^7.0.0",
        "eslint-plugin-react-refresh": "^0.4.24",
        "eslint-plugin-storybook": "9.1.13",
        "generate-react-cli": "9.1.0",
        "globals": "^16.4.0",
        "jest": "30.2.0",
        "postcss": "8.5.6",
        "prettier": "3.6.2",
        "prettier-plugin-sh": "^0.18.0",
        "prettier-plugin-tailwindcss": "^0.7.1",
        "sass": "1.93.2",
        "storybook": "9.1.13",
        "swr": "2.3.6",
        "tailwindcss": "4.1.15",
        "typescript": "~5.9.3",
        "typescript-eslint": "^8.46.2",
        "vite": "7.1.11",
        "vite-tsconfig-paths": "5.1.4",
        "wireit": "0.14.12",
        "zod": "4.1.12"
    },
    "files": [
        "dist/"
    ],
    "name": "serenity-frontend",
    "private": true,
    "scripts": {
        "build": "wireit",
        "build:storybook": "wireit",
        "commit": "npx git-cz@latest",
        "create:component": "generate-react c",
        "create:widget": "generate-react c --type=widget",
        "dev": "wireit",
        "full-dev": "wireit",
        "hook:pre-push": "wireit",
        "lint": "wireit",
        "preinstall": "npx only-allow pnpm",
        "preview": "vite preview",
        "push": "wireit",
        "release": "wireit",
        "start": "wireit",
        "storybook": "wireit",
        "type-check": "wireit"
    },
    "type": "module",
    "version": "0.0.0",
    "wireit": {
        "build": {
            "command": "just build app",
            "dependencies": [
                "type-check",
                "lint"
            ]
        },
        "build:storybook": {
            "command": "just build storybook"
        },
        "dev": {
            "command": "just dev",
            "service": true
        },
        "full-dev": {
            "dependencies": [
                "dev",
                "storybook"
            ]
        },
        "lint": {
            "command": "just lint"
        },
        "push": {
            "command": "just push",
            "dependencies": [
                "build"
            ]
        },
        "release": {
            "dependencies": [
                "release:version",
                "release:push"
            ]
        },
        "release:push": {
            "command": "just release push",
            "dependencies": [
                "release:version"
            ]
        },
        "release:version": {
            "command": "just release version"
        },
        "start": {
            "command": "just start",
            "service": true
        },
        "storybook": {
            "command": "just storybook",
            "service": true
        },
        "type-check": {
            "command": "just type-check"
        }
    }
}
